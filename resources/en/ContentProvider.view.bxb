input-view {
  match: ContentProviderDetails (this)
  message {
    template (Choose your streaming content provider. Please note, some providers may not provide content in your locale. ðŸ‘‡) {
      speech (Choose your streaming content provider.)
    }
  }
  render {
    selection-of (this) {
      where-each (item) {
        layout-macro (content-provider) {
          param (provider) {
            expression (item)
          }
        }
      }
    }
  }
}
